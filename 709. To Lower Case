class Solution {
    public String toLowerCase(String str) {
        //return str.toLowerCase();
        char c[]=str.toCharArray();
        for(int i=0;i<c.length;i++){
            if(c[i]>='A' &&c[i]<='Z')
            {
               // c[i]=(char)(c[i]-'A'+'a');
                c[i]+=32;
            }
        }
return new String(c);}
}
